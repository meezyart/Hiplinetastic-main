{
    "name": "eleventastic",
    "version": "1.0.0",
    "description": "A boilerplate for eleventy websites",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/maxboeck/eleventastic"
    },
    "author": "Max BÃ¶ck",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-img": "^0.8.2",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.1.1",
        "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
        "@babel/core": "^7.13.14",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.12",
        "@dogstudio/highway": "^2.2.1",
        "@sanity/block-content-to-html": "^2.0.0",
        "@sanity/client": "^2.13.0",
        "@sanity/image-url": "^0.140.22",
        "alpinejs": "^3.2.4",
        "autoprefixer": "^10.3.2",
        "babel-loader": "^8.2.2",
        "clean-css": "^5.1.2",
        "critical": "^3.0.0",
        "cssesc": "^3.0.0",
        "del-cli": "^3.0.1",
        "focus-trap": "^6.3.0",
        "focus-visible": "^5.2.0",
        "get-video-id": "^3.1.7",
        "get-youtube-id": "^1.0.1",
        "groq": "^2.13.0",
        "gsap": "^3.7.1",
        "html-minifier": "^4.0.0",
        "inobounce": "^0.2.0",
        "lazysizes": "^5.3.2",
        "luxon": "^1.26.0",
        "markdown-it": "^12.0.4",
        "memfs": "^3.2.0",
        "node-sass": "^5.0.0",
        "npm-run-all": "^4.1.5",
